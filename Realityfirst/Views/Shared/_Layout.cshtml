<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RealityFirst</title>
    <link rel="icon" href="~/img/logo_transparent.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
    <style>
        .offcanvas-backdrop.fade{
            background:none;
        }
		html,body{
			background-image: url('https://www.xtrafondos.com/descargar.php?id=3472&resolucion=2880x1800');
			background-size: cover;
			background-repeat: no-repeat;
			height: 100%;
			font-family: 'Numans', sans-serif;
            background-attachment: fixed;
		}
    </style>
</head>
<body> 
     <header>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark" width="100">
    <div class="container-fluid">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/img/logo_transparent.png" width="75px"/>RealityFirst</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
        </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active text-white" aria-current="page" asp-area="" asp-controller="Home" asp-action="Contacto" >Contacto</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active text-white" aria-current="page" asp-area="" asp-controller="Artista" asp-action="Artista">Artistas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active text-white" aria-current="page" asp-area="" asp-controller="Evento" asp-action="busquedaeventos">Eventos</a>
                </li>              
      
            </ul> 
            <div class="btn btn-dark">
                <div class="btn btn-secundary payment">
                <a href="/Home/Login" class="btn text-white">Ingresar</a>
                </div>            

             </div>
      </div>
    
            
    </nav>
    </header>

    <br><br><br><br>            

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="bg-dark text-center text-white" <a asp-area="" asp-controller="Privacy" asp-action="Privacy" asp-controller="Eventos" asp-action="BusquedaEventos" asp-action="Eventos"></a>
  
  <!-- Copyright -->
  <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">

  </div>
  <!-- Copyright -->
</footer>
    <br>

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vex-js/4.1.0/js/vex.min.js" integrity="sha512-qU0PChDrIKPBhT0pJpD5yw3HtJYekKiJa1cl92tjKCCwLNOj5AZ/PGzxI5qIDmyIbk/6nK3vrEWshYozID56UA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.slim.min.js" integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI=" crossorigin="anonymous"></script>
    @RenderSection("Styles", required: false)
    @RenderSection("Scripts", required: false)
    @RenderSection("Filtrar", required: false)
</body>
</html>